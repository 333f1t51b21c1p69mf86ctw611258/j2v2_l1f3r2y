<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false" auto-import="false">
	<import class="com.dasannetworks.vn.sb.model.Contact" />
	<import class="com.dasannetworks.vn.sb.model.Device" />
	<import class="com.dasannetworks.vn.sb.model.HandOverDevice" />
	<import class="com.dasannetworks.vn.sb.model.HandOverForm" />
	<import class="com.dasannetworks.vn.sb.model.PurchaseOrder" />
	<class name="com.dasannetworks.vn.sb.model.impl.ContactImpl" table="tms_Contact">
		<id name="id" column="id_" type="long">
			<generator class="assigned" />
		</id>
		<property name="uuid" type="com.liferay.portal.dao.orm.hibernate.StringType" column="uuid_" />
		<property name="firstname" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="lastname" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="telephone" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="email" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
	<class name="com.dasannetworks.vn.sb.model.impl.DeviceImpl" table="tms_Device">
		<id name="deviceId" type="long">
			<generator class="assigned" />
		</id>
		<property name="groupId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="companyId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="createDate" type="org.hibernate.type.TimestampType" />
		<property name="modifiedDate" type="org.hibernate.type.TimestampType" />
		<property name="modelName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="serialNumber" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="hardwareRevision" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="softwareRevision" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="manufacturerSerial" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="macAddress" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="partNumber" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="specification" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="factoryOut" type="org.hibernate.type.TimestampType" />
		<property name="warrantyStartDate" type="org.hibernate.type.TimestampType" />
		<property name="warrantyEndDate" type="org.hibernate.type.TimestampType" />
		<property name="purchaseOrderId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
	</class>
	<class name="com.dasannetworks.vn.sb.model.impl.HandOverDeviceImpl" table="tms_HandOverDevice">
		<composite-id name="primaryKey" class="com.dasannetworks.vn.sb.service.persistence.HandOverDevicePK">
			<key-property name="handOverFormId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
			<key-property name="deviceId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		</composite-id>
		<property name="companyId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="createDate" type="org.hibernate.type.TimestampType" />
		<property name="modifiedDate" type="org.hibernate.type.TimestampType" />
		<property name="faultNote" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="status" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
	<class name="com.dasannetworks.vn.sb.model.impl.HandOverFormImpl" table="tms_HandOverForm">
		<id name="handOverFormId" type="long">
			<generator class="assigned" />
		</id>
		<property name="companyId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="createDate" type="org.hibernate.type.TimestampType" />
		<property name="modifiedDate" type="org.hibernate.type.TimestampType" />
		<property name="customerCode" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="contactPoint" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="email" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="phone" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="location" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="personInCharge" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="handOverFormNumber" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="note" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="assignDate" type="org.hibernate.type.TimestampType" />
		<property name="estimatedReturnDate" type="org.hibernate.type.TimestampType" />
	</class>
	<class name="com.dasannetworks.vn.sb.model.impl.PurchaseOrderImpl" table="tms_PurchaseOrder">
		<id name="purchaseOrderId" type="long">
			<generator class="assigned" />
		</id>
		<property name="companyId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="createDate" type="org.hibernate.type.TimestampType" />
		<property name="modifiedDate" type="org.hibernate.type.TimestampType" />
		<property name="customer" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="signDate" type="org.hibernate.type.TimestampType" />
		<property name="purchaseOrderNumber" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
</hibernate-mapping>