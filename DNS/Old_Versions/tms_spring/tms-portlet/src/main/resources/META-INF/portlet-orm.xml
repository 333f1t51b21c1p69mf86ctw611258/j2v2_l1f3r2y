<?xml version="1.0"?>

<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd">
	<persistence-unit-metadata>
		<xml-mapping-metadata-complete/>
		<persistence-unit-defaults>
			<access>PROPERTY</access>
		</persistence-unit-defaults>
	</persistence-unit-metadata>
	<mapped-superclass class="com.dasannetworks.vn.sb.model.impl.DeviceModelImpl">
		<attributes>
			<id name="deviceId" />
			<basic name="groupId" />
			<basic name="companyId" />
			<basic name="userId" />
			<basic name="userName" />
			<basic name="createDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="modifiedDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="modelName" />
			<basic name="serialNumber" />
			<basic name="hardwareRevision" />
			<basic name="softwareRevision" />
			<basic name="manufacturerSerial" />
			<basic name="macAddress" />
			<basic name="partNumber" />
			<basic name="specification" />
			<basic name="factoryOut">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="uploadFileId" />
			<transient name="modelAttributes" />
			<transient name="primaryKey" />
			<transient name="primaryKeyObj" />
			<transient name="userUuid" />
		</attributes>
	</mapped-superclass>
	<mapped-superclass class="com.dasannetworks.vn.sb.model.impl.UploadFileModelImpl">
		<attributes>
			<id name="uploadFileId" />
			<basic name="groupId" />
			<basic name="companyId" />
			<basic name="userId" />
			<basic name="userName" />
			<basic name="createDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="modifiedDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="status" />
			<basic name="fileName" />
			<transient name="modelAttributes" />
			<transient name="primaryKey" />
			<transient name="primaryKeyObj" />
			<transient name="userUuid" />
		</attributes>
	</mapped-superclass>
	<entity class="com.dasannetworks.vn.sb.model.impl.DeviceImpl" name="Device">
		<table name="tms_Device" />
		<attributes />
	</entity>
	<entity class="com.dasannetworks.vn.sb.model.impl.UploadFileImpl" name="UploadFile">
		<table name="tms_UploadFile" />
		<attributes />
	</entity>
</entity-mappings>